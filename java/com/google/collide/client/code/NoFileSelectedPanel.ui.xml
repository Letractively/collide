<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder">
  <ui:with field="css"
    type="com.google.collide.client.code.NoFileSelectedPanel.Css" />

  <div ui:field="base" class="{css.base}">
    <div class="{css.center}">
      <div class="{css.logo}"></div>
      <br />
      <p class="{css.text}">Welcome to your branch.</p>
      <br />
      <p ui:field="message" class="{css.text} {css.bigger}"></p>
    </div>
  </div>
</ui:UiBinder>
